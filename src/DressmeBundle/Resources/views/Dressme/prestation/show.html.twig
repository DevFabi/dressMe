{% extends 'base.html.twig' %}

{% block body %}
    <h1>Prestation</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prestation.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ prestation.libelle }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ prestation.prix }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('prestation_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('prestation_edit', { 'id': prestation.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
